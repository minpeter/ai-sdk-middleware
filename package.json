{
  "name": "@ai-sdk-tool/middleware",
  "version": "0.0.3",
  "description": "Collection of reusable AI SDK middlewares",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/minpeter/ai-sdk-middleware"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./reasoning-parser": {
      "types": "./dist/reasoning-parser.d.ts",
      "import": "./dist/reasoning-parser.js",
      "require": "./dist/reasoning-parser.cjs"
    },
    "./disk-cache": {
      "types": "./dist/disk-cache.d.ts",
      "import": "./dist/disk-cache.js",
      "require": "./dist/disk-cache.cjs"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "pnpm clean && tsup --tsconfig tsconfig.build.json",
    "build:watch": "pnpm clean && tsup --watch --tsconfig tsconfig.build.json",
    "clean": "rm -rf dist *.tsbuildinfo",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "check:biome": "biome check",
    "check:types": "pnpm run typecheck",
    "check": "pnpm run check:biome && pnpm run check:types",
    "fmt:biome": "biome check --write",
    "fmt": "pnpm run fmt:biome",
    "changeset": "changeset",
    "ci:release": "pnpm build && changeset publish",
    "ci:version": "changeset version"
  },
  "dependencies": {
    "@ai-sdk/provider": "3.0.8"
  },
  "devDependencies": {
    "@biomejs/biome": "2.4.3",
    "@changesets/cli": "2.29.8",
    "@types/node": "^25.3.0",
    "@vitest/coverage-v8": "^4.0.18",
    "tsup": "^8.5.1",
    "typescript": "5.9.3",
    "ultracite": "7.2.3",
    "vitest": "^4.0.18"
  },
  "keywords": [
    "ai",
    "sdk",
    "middleware"
  ],
  "author": "",
  "license": "Apache-2.0",
  "packageManager": "pnpm@9.14.4",
  "engines": {
    "node": ">=18"
  }
}
