{
  "name": "@ai-sdk-tool/middleware",
  "version": "0.0.2",
  "description": "Collection of reusable AI SDK middlewares",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/minpeter/ai-sdk-middleware"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./reasoning-parser": {
      "types": "./dist/reasoning-parser.d.ts",
      "import": "./dist/reasoning-parser.js",
      "require": "./dist/reasoning-parser.cjs"
    },
    "./disk-cache": {
      "types": "./dist/disk-cache.d.ts",
      "import": "./dist/disk-cache.js",
      "require": "./dist/disk-cache.cjs"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "pnpm clean && tsup --tsconfig tsconfig.build.json",
    "build:watch": "pnpm clean && tsup --watch --tsconfig tsconfig.build.json",
    "clean": "rm -rf dist *.tsbuildinfo",
    "typecheck": "tsc --noEmit",
    "test": "vitest run"
  },
  "dependencies": {
    "@ai-sdk/provider": "3.0.2"
  },
  "devDependencies": {
    "@types/node": "^25.0.7",
    "@vitest/coverage-v8": "^4.0.17",
    "tsup": "^8.5.1",
    "typescript": "5.9.3",
    "vitest": "^4.0.17"
  },
  "keywords": [
    "ai",
    "sdk",
    "middleware"
  ],
  "author": "",
  "license": "Apache-2.0",
  "packageManager": "pnpm@9.14.4",
  "engines": {
    "node": ">=18"
  }
}
